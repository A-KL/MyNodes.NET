{
  "FirstRun": false,
  "Gateway": {
    "SerialGateway": {
      "Enable": false,
      "SerialPort": "COM3"
    },
    "EthernetGateway": {
      "Enable": false,
      "GatewayIP": null,
      "GatewayPort": 0
    },
    "EnableAutoAssignId": true,
    "EnableMessagesLog": true,
    "LogState": true,
    "LogMessages": true
  },
  "DataBase": {
    "Enable": true,
    "UseInternalDb": true,
    "ExternalDbConnectionString": "Data Source=.\\sqlexpress;Initial Catalog=MyNetSensors;Integrated Security=True",
    "WriteInterval": 5000,
    "LogState": true
  },
  "NodesEngine": {
    "Enable": true,
    "UpdateInterval": 10,
    "LogEngine": true,
    "LogNodes": true
  },
  "WebServer": {
    "Enable": true,
    "Rules": {
      "AllowRegistrationOfNewUsers": true
    },
    "Debug": false,
    "Logging": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Verbose",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  }
}